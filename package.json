{
  "name": "joblahoma",
  "version": "1.0.0",
  "private": true,
  "description": "Techlahoma Job Board",
  "main": "src/server/index.js",
  "scripts": {
    "build": "NODE_PATH=./src NODE_ENV=production ./node_modules/browserify/bin/cmd.js src/client/index.js -o public/js/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "minify": "./node_modules/uglify-js/bin/uglifyjs public/js/bundle.js --compress --mangle -o public/js/bundle.js",
    "start": "npm run build && npm run minify && source .env && NODE_ENV=production node --harmony_default_parameters --harmony-destructuring src/server/index.js",
    "start:dev": "npm run build && source .env && nodemon --delay 2 --harmony_default_parameters --harmony-destructuring src/server/index.js",
    "watch": "fsmonitor -d 'src/' '!server' npm run build"
  },
  "author": "Vance Lucas",
  "license": "",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "joi": "^8.0.5",
    "knex": "^0.9.0",
    "marked": "^0.3.5",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^1.2.1",
    "pg": "^4.4.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "frisby": "git://github.com/vlucas/frisby.git#v2"
  },
  "browser": {
    "server/sdk": "./src/client/sdk"
  }
}
